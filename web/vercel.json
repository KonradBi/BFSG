{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "bfsg.vercel.app" }],
      "destination": "https://bfsg-webcheck.de/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.bfsg-webcheck.de" }],
      "destination": "https://bfsg-webcheck.de/$1",
      "permanent": true
    }
  ]
}
